      <geometry>
        <mesh filename="daes/base_rot.dae"/>
        <!--- <cylinder length="0.16" radius="0.02"/>  <! 0.16 0.02 --> 
      </geometry>
	<origin xyz = "0.0 0 0 " rpy = " 0 0 1.57"/>  <!--- 0.0 0 -0.04 -->
	<material name = "inwhite">
		<color  rgba = "0.8 0.8 0.8 0.8"/>
	</material>
    </visual>
  </link>

  <link name="link_1">
