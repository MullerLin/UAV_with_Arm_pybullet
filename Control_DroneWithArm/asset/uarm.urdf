    <parent link="link_2"/>
    <child link="add_4"/>
	<origin xyz= "0 0 -0.0136" rpy = " 0 0 0" />   <!--- 0.02 0  0 -->
   </joint>

   <joint name="link_3_to_add_2" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-1.57" upper = "2.356" velocity = "0"/>
    <parent link="link_3"/>
    <child link="add_2"/>
	<origin xyz= "0.02022 -0.03367 -0.01751" rpy = " 0 0 1.57" />   <!--- 0.02 0  0 -->
   </joint>

  <joint name="base_rot_to_add_3" type="revolute">

	<axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-1.57" upper = "3.1416" velocity = "0"/>
    <parent link="base_rot"/>
    <child link="add_3"/>
	<origin xyz= "-0.01 0.022 0.09783" rpy = " -1.57  0 3.1416"/>  <!--- 0.0212 0 0.006 -->
  </joint>


 <joint name="link_2_to_add_5" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-1.57" upper = "1.57" velocity = "0"/>
    <parent link="link_2"/>
    <child link="add_5"/>
	<origin xyz= "-0.054 0.005 0.0129" rpy = " 0 0 0" />   <!--- 0.02 0  0 -->
   </joint>
</robot>

