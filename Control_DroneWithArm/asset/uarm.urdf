  <joint name="base_to_base_rot" type="revolute">

	<axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "0.0" upper = "3.14159" velocity = "0"/>
	<parent link="base"/>
	<child link="base_rot"/>
	<origin xyz= "0 0 0.032" rpy = "0 0 0"/>  <!--- 0 0 0.10645 -->
  </joint>

  <joint name="base_rot_to_link_1" type="revolute">

	<axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-0.5236" upper = "2.356" velocity = "0"/>
    <parent link="base_rot"/>
    <child link="link_1"/>
	<origin xyz= "0.02117 0 0.07345" rpy = "  1.57  0 0"/>  <!--- 0.0212 0 0.006 -->
  </joint>

  <joint name="link_1_to_link_2" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-2.53" upper = "2.53" velocity = "0"/>
    <parent link="link_1"/>
    <child link="link_2"/>
	<origin xyz= "0.14825 0  0 " rpy = " 0 0 0"/> <!--- 0.14 0  0 -->
  </joint>

  <joint name="link_2_to_link_3" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-2.53" upper = "2.53" velocity = "0"/>
    <parent link="link_2"/>
    <child link="link_3"/>
	<origin xyz= "0.16 0.005 0" rpy = " 0 0 1.57" />   <!--- 0.16 0  0 -->
  </joint>

  <joint name="link_3_to_link_end" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "0.0" upper = "3.1416" velocity = "0"/>
    <parent link="link_3"/>
    <child link="link_end"/>
	<origin xyz= "-0.009 -0.0348 0" rpy = " 0 1.57 0" />   <!--- 0.02 0  0 -->
   </joint>

  <joint name="link_1_to_add_1" type="revolute">

	 <axis xyz="0 0 1"/>
	<limit effort = "1000.0" lower = "-3.1416" upper = "0" velocity = "0"/>
    <parent link="link_1"/>
    <child link="add_1"/>
	<origin xyz= "0 0 0.00875" rpy = " 0 0 3.1416" />   <!--- 0.02 0  0 -->
   </joint>

  <joint name="link_2_to_add_4" type="revolute">

	 <axis xyz="0 0 1"/>
